.decl Check(n1:number, n2:number, n3:number, n4:number, n5:number, n6: number)
.input Check()
.decl In(n1:number, n2:number, n3:number, n4:number, n5:number, n6: number, in:number)
.input In()
.decl A(l:number, o:number)
.output A()

A(1,i) :- Check(a, b, c, d, e, f), In(a, b, c, d, e, f, i).
A(2,i) :- Check(a, _, _, d, e, f), In(a, _, _, d, e, f, i).
A(3,i) :- Check(a, b, _, d, e, f), In(a, b, _, d, e, f, i).
A(4,i) :- Check(a, _, _, _, e, f), In(a, _, _, _, e, f, i).
A(5,i) :- Check(a, b, _, _, _, f), In(a, b, _, _, _, f, i).
A(6,i) :- Check(a, _, _, _, _, _), In(a, _, _, _, _, _, i).
A(7, i) :- Check(a, b, _, d, e, f), In(a, b, _, d, e, f, i).
A(8, i) :- Check(a, b, _, _, e, f), In(a, b, _, _, e, f, i).
A(9, i) :- Check(a, b, _, _, _, f), In(a, b, _, _, _, f, i).
A(10, i) :- Check(a, b, _, _, _, _), In(a, b, _, _, _, _, i).
A(11, i) :- Check(a, b, c, _, e, f), In(a, b, c, _, e, f, i).
A(12, i) :- Check(a, _, c, _, _, f), In(a, _, c, _, _, f, i).
A(13, i) :- Check(a, b, c, _, _, _), In(a, b, c, _, _, _, i).
A(14, i) :- Check(a, b, c, d, _, f), In(a, b, c, d, _, f, i).
A(15, i) :- Check(a, _, c, d, _, _), In(a, _, c, d, _, _, i).



