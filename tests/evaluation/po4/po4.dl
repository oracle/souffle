.decl Check(n1:number, n2:number, n3:number, n4:number, n5:number, n6: number)
.input Check()
.decl In(n1:number, n2:number, n3:number, n4:number, n5:number, n6: number, in:number)
.input In()
.decl A(l:number, o:number)
.output A()

A(1,i) :- Check(a, b, c, d, e, f), In(a, b, c, d, e, f, i).
A(2,i) :- Check(a, b, c, d, _, _), In(a, b, c, d, _, _, i).
A(3,i) :- Check(a, _, c, d, _, _), In(a, _, c, d, _, _, i).
A(4,i) :- Check(a, b, _, d, _, _), In(a, b, _, d, _, _, i).
A(5,i) :- Check(a, b, c, _, _, _), In(a, b, c, _, _, _, i).
A(6,i) :- Check(_, _, c, d, _, _), In(_, _, c, d, _, _, i).
A(7, i) :- Check(a, b, _, _, _, _), In(a, b, _, _, _, _, i).
A(8, i) :- Check(a, _, _, _, _, _), In(a, _, _, _, _, _, i).
A(9, i) :- Check(_, b, _, _, _, _), In(_, b, _, _, _, _, i).
