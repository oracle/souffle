.comp A {
    .decl Rel(x:number) overridable
.output Rel()
    .comp AA {
        Rel(0).
    }

    .init aa = AA

    Rel(1).
}

.comp B : A {
    .override Rel
    .comp BB {
        Rel(2).
    }
    .init bb = BB
    Rel(3).
}

.init a = A
.init b = B