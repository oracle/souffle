//
// Check semantics of binary relations
//

.type A

.decl Y (a:A, b:A)
.decl Z (a:A, b:A, c:A)
.output Z ()

Y("a","b").
Y("c","d").

Z(a,b, cat(cat(a,b), a)) :- Y(a,b).
