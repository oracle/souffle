// x  = 10;
// y = 11;
// if(x - y> 0)
//    z = z
// else
//    z = z * -1
//
//   assert(z >= 0)
//

.decl loop(i:number, x:number, y:number, z:number)
.decl R()
.output R()

loop(0, 0, 0, 0).
loop(1, 10, y, z) :- loop(0, _, y, z).
loop(2, x, y, z) :- loop(1, x, _, _), loop(0, _, y, z), x - z > 0.
loop(3, x, y, z*-1) :- loop(1, x, y, z), x - z <= 0.
loop(4, x, y, z) :- loop(2, x, y, z).
loop(4, x, y, z) :- loop(3, x, y, z).
R() :- loop(4, _, _, z), z >= 0.
