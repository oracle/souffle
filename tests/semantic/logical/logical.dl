.decl A(x:number)
A(0).
A(1).

.decl r_land(x:number, y:number, z:number)
.output r_land()
r_land(x land y, x, y) :- A(x), A(y).

.decl r_lor(x:number, y:number, z:number)
.output r_lor()
r_lor(x lor y, x, y) :- A(x), A(y).

.decl r_lnot(x:number, y:number)
.output r_lnot()
r_lnot(lnot x, x) :- A(x).

